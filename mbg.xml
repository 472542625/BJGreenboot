<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3">
		<!-- 配置数据库连接 useSSL=false-->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.postgresql.Driver"
			connectionURL="jdbc:postgresql://localhost:5432/bjgreen?characterEncoding=UTF-8"
			userId="postgres" password="123456">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<!-- 指定javabean生成的位置 -->
		<javaModelGenerator targetPackage="com.lc.test.test01.entity"
			targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- 指定sql映射文件生成的位置-->
		<sqlMapGenerator targetPackage="com.lc.test.test01.dao" targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- 指定dao接口生成的位置，mapper的接口-->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.lc.test.test01.dao" targetProject=".\src\main\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>
		<!-- table表指定每个表的生成策略 -->

		<table tableName="daxing_tree" domainObjectName="Tree">

		</table>



		 




	</context>
</generatorConfiguration>